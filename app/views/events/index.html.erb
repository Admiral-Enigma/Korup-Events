<%= render partial: "shared/navbar", locals: {page: "home"} %>
<h1>HEy</h1>

<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>#</th>
      <th>Navn</th>
      <th>Dato</th>
      <th>Pris</th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
      <% if event.accepted == true %>
      <tr>
        <td><%= event.id %></td>
        <td><%= event.title %></td>
        <td><%= event.date %></td>
        <td><%= event.price %></td>
      </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
