<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>#</th>
      <th>Title</th>
      <th>Dato</th>
      <th>Pris</th>
      <th>Navn</th>

    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
    <tr>
      <td><%= event.id  %></td>
      <td><%= event.title  %></td>

      <td><%= event.date  %></td>
      <td><%= event.price %></td>
      <td><%= event.contact_name %></td>
    </tr>
    <% end %>
  </tbody>

</table>
